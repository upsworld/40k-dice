{
  "env": {
    "browser": true
  },
	"globals": { },
  "extends": "eslint:recommended",
  "rules": {
    "camelcase": 0,
    "no-empty": [2, { "allowEmptyCatch": true }],
    "no-undef": 0,
    "no-console": 0,
    "no-unused-vars": 0,
    "no-constant-condition": ["error", { "checkLoops": false }]
  }
}
